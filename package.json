{
	"name": "react-ts-antd-starter",
	"version": "0.0.1",
	"description": "Starter kit for React+TS+ANTD",
	"main": "index.js",
	"scripts": {
		"build": "npm run clean && webpack --config build/webpack.config.js --mode production --progress",
		"analyze": "npm run clean && webpack --config build/webpack.config.js --mode production --analyze --progress",
		"dev": "npm run clean && webpack --config build/webpack.config.js --mode development --progress",
		"start": "webpack-dev-server --config build/webpack.config.js --mode development",
		"clean": "rimraf ./dist",
		"eslint": "eslint --cache --ext .ts,.tsx src",
		"eslint:dump": "eslint --print-config src/.eslintrc.json"
	},
	"keywords": [
		"react",
		"typescript",
		"antd",
		"less",
		"webpack"
	],
	"author": "lexey111",
	"license": "ISC",
	"dependencies": {
		"@ant-design/icons": "^4.7.0",
		"antd": "^4.0.0",
		"react": "^16.12.0",
		"react-dom": "^16.12.0",
		"react-router": "^5.1.2",
		"react-router-dom": "^5.1.2"
	},
	"devDependencies": {
		"@typescript-eslint/eslint-plugin": "^2.15.0",
		"@typescript-eslint/parser": "^2.15.0",
		"antd-dayjs-webpack-plugin": "0.0.8",
		"copy-webpack-plugin": "^5.1.1",
		"eslint": "^6.8.0",
		"eslint-loader": "^3.0.3",
		"eslint-plugin-optimize-regex": "^1.1.7",
		"eslint-plugin-promise": "^4.2.1",
		"eslint-plugin-react": "^7.17.0",
		"eslint-plugin-react-hooks": "^2.3.0",
		"eslint-plugin-sonarjs": "^0.5.0",
		"file-loader": "^5.0.2",
		"less": "^3.10.3",
		"less-loader": "^5.0.0",
		"lorem-ipsum": "^2.0.3",
		"optimize-css-assets-webpack-plugin": "^5.0.3",
		"rimraf": "^3.0.0",
		"ts-import-plugin": "^1.6.2",
		"ts-loader": "^6.2.1",
		"typescript": "^3.7.4",
		"webpack": "^4.41.5",
		"webpack-bundle-analyzer": "^3.6.0",
		"webpack-cli": "^3.3.10",
		"webpack-dev-server": "^3.10.1"
	}
}
